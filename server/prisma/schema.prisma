generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

enum Role {
    MANUFACTURER
    DISTRIBUTOR
}

model User {
    address     String   @id
    name        String
    description String
    role        Role
    createdAt   DateTime @default(now())
    updatedAt   DateTime @updatedAt
}
